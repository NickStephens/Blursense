#include BlurClient.repy

dy_import_module_symbols("BlurClient.repy")

class GPSRequest (BlurClient):
	def get (self, params):
		lat = self.mRPCClient.send_request ('GPSLocationSensor.latitude', ())
		lon = self.mRPCClient.send_request ('GPSLocationSensor.longitude', ())

		return ( lat, lon )

	def name ( self ):
		return "gps"
